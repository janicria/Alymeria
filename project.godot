; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="System Mycosis"
config/version="Pre-alpha 0.1 - Development heaven when?"
run/main_scene="res://scences/ui/menus/main_menu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Alymeria"
config/features=PackedStringArray("4.3", "GL Compatibility")
run/max_fps=60
config/icon="res://assets/characters/tile_0122.png"

[audio]

buses/default_bus_layout="res://assets/misc/default_bus_layout.tres"

[autoload]

Events="*res://global/events.gd"
MusicPlayer="*res://global/music_player.tscn"
SFXPlayer="*res://global/sfx_player.tscn"
Data="*res://global/data.gd"
ConsoleDataManager="*res://addons/simply-console/singletons/console_data_manager.gd"

[debug]

file_logging/enable_file_logging=true
file_logging/log_path="user://logs/info.log"
file_logging/max_log_files=4
gdscript/warnings/unused_signal=0
gdscript/warnings/untyped_declaration=1
gdscript/warnings/integer_division=0
gdscript/warnings/int_as_enum_without_cast=0
gdscript/warnings/inference_on_variant=1
gdscript/warnings/native_method_override=1

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/resizable=false
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="viewport"
mouse_cursor/custom_image="res://assets/misc/cursor.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg", "res://addons/script-ide/plugin.cfg", "res://addons/simply-console/plugin.cfg")

[gui]

theme/custom="res://assets/misc/main_theme.tres"
theme/custom_font="res://assets/misc/font.ttf"

[importer_defaults]

texture={
"compress/channel_pack": 0,
"compress/hdr_compression": 1,
"compress/high_quality": false,
